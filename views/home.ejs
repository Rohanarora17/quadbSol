<body class="add" >
  <header style="padding: 20px 30px 0px;">
    <div id="head-section" class="align-items-center justify-content-sm-center row">
      <div class="ownwer text-center padding-none col-12 col-sm-12 col-md-2 col-lg-4">
        <h5>
          <h2 style="display: inline-block;" class="head">HODLINFO</h2>.com
        </h5>
      </div>

      <style>
        .average-subText {
          
          opacity: .45;
          font-weight: 300;
          font-size: 16px;
          letter-spacing: .75px;
        }
      
        .subText-heading {
          font-size: 28px;
        }
      </style>
      

      <div class="butt-group text-center padding-none col-12 col-sm-12 col-md-2 col-lg-4">
        <div class="btn-group">
          <button type="button" aria-haspopup="true" aria-expanded="false"
            class="header-button dropdown-toggle btn btn-secondary">
            INR</button>
          <div tabindex="-1" role="menu" aria-hidden="true" class="dropdown-menu"><a value="INR" tabindex="0"
              role="menuitem" class="dropdown-item" href="/BTC-INR">INR</a>
          </div>
        </div>
        <div class="btn-group">
          <button type="button" aria-haspopup="true" aria-expanded="true" id = "dropdownMenuButton1" class="header-button dropdown-toggle btn btn-secondary" data-bs-toggle="dropdown">
            BTC
          </button>
          
          <div tabindex="-1" role="menu" aria-hidden="false" class="dropdown-menu"  aria-labelledby="dropdownMenuButton1"
